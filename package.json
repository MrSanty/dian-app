{
  "name": "dian-app",
  "private": true,
  "version": "0.0.0",
  "main": "./public/index.js",
  "author": "mrsanty",
  "license": "MIT",
  "description": "A simple electron app with react and vite",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "electron:dev": "electron .",
    "electron:build": "electron-builder"
  },
  "dependencies": {
    "@types/pouchdb": "^6.4.0",
    "events": "^3.3.0",
    "pouchdb": "^8.0.1",
    "pouchdb-browser": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "electron": "^23.1.3",
    "electron-builder": "^23.6.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  },
  "build": {
    "appId": "com.example.my-electron-app",
    "productName": "dianApp",
    "directories": {
      "output": "build"
    },
    "win": {
      "target": "portable",
      "artifactName": "dianApp.${ext}",
      "extraResources": [
        {
          "from": "dist",
          "to": "."
        }
      ]
    }
  }
}